{
  "name": "polaris",
  "private": true,
  "main": "./index.js",
  "scripts": {
    "start": "expo start",
    "web": "expo start --web",
    "web:ci": "expo start --web &",
    "ios": "expo start -c --ios",
    "android": "expo start -c --android",
    "start:docs": "docsify serve ./docs --port 4000",
    "build:web": "expo build:web",
    "build:ios": "expo build:ios",
    "build:android": "expo build:android",
    "build:local:ios": "turtle build:ios --type=simulator --output=build/polaris.tgz && tar xzf build/polaris.tgz -C build",
    "build:local:android": "turtle build:android --type=apk --output=build/polaris.apk",
    "eject": "expo eject",
    "lint": "eslint --ext .js,.jsx .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:ci": "jest --ci",
    "prestorybook": "rnstl",
    "storybook:web": "start-storybook -p 9001 -c ./storybook/.storybook-web",
    "storybook:ios": "run-p prestorybook && cross-env RUN_STORYBOOK=true expo start -c --ios",
    "storybook:android": "run-p prestorybook && cross-env RUN_STORYBOOK=true expo start -c --android",
    "test:watch": "jest --watch",
    "checks": "run-p lint:fix test",
    "e2e:web:open": "cypress open",
    "e2e:web:run": "cypress run",
    "e2e:web:run:ci": "wait-on http://localhost:19006 && npm run e2e:web:run -- --config video=false",
    "create:env": "node -r fs -e \"fs.copyFileSync('.env.sample', '.env', fs.constants.COPYFILE_EXCL)\"",
    "push:generate:web": "web-push generate-vapid-keys",
    "push:server:start": "node server.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.4",
    "@react-native-community/async-storage": "~1.11.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/picker": "1.6.0",
    "@react-navigation/bottom-tabs": "^5.7.1",
    "@react-navigation/drawer": "^5.12.3",
    "@react-navigation/native": "^5.6.1",
    "@react-navigation/stack": "^5.14.3",
    "date-fns": "^2.17.0",
    "dotenv": "^8.2.0",
    "expo": "^38.0.0",
    "expo-auth-session": "~1.4.0",
    "expo-barcode-scanner": "~8.2.1",
    "expo-camera": "~8.3.1",
    "expo-constants": "~9.1.1",
    "expo-localization": "~8.2.1",
    "expo-notifications": "~0.3.3",
    "expo-permissions": "~9.0.1",
    "expo-sharing": "~8.2.1",
    "expo-status-bar": "^1.0.0",
    "expo-web-browser": "~8.3.1",
    "fastify": "^3.0.0",
    "fastify-cors": "^5.2.0",
    "husky": "^4.3.8",
    "i18next": "^20.1.0",
    "i18next-browser-languagedetector": "^6.0.1",
    "jwt-decode": "^3.1.2",
    "prop-types": "^15.7.2",
    "react": "16.11.0",
    "react-dom": "16.13.0",
    "react-i18next": "^11.8.5",
    "react-native": "https://github.com/expo/react-native/archive/sdk-38.0.2.tar.gz",
    "react-native-elements": "^3.3.2",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-maps": "^0.27.1",
    "react-native-reanimated": "~1.9.0",
    "react-native-safe-area-context": "~3.0.7",
    "react-native-screens": "~2.9.0",
    "react-native-side-menu": "^1.1.3",
    "react-native-svg": "12.1.0",
    "react-native-web": "~0.11.7",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.1.1",
    "styled-media-query": "^2.1.2",
    "web-push": "^3.4.4"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.{css,md,json}": [
      "prettier --write"
    ]
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src",
        "./storybook/story-components"
      ],
      "pattern": "**/stories/{*.stories.common,*.stories.native}.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.12.11",
    "@expo/webpack-config": "^0.12.52",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-ondevice-actions": "^5.3.19",
    "@storybook/addon-ondevice-knobs": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^6.0.28",
    "@storybook/react-native": "^5.3.23",
    "@svgr/webpack": "^5.4.0",
    "@testing-library/jest-native": "^3.1.0",
    "@testing-library/react": "^10.4.5",
    "@testing-library/react-hooks": "^3.3.0",
    "@testing-library/react-native": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-inline-dotenv": "^1.5.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-expo": "^8.2.3",
    "cross-env": "^7.0.3",
    "cypress": "^4.9.0",
    "docsify-cli": "^4.4.2",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.0.5",
    "eslint-plugin-standard": "^5.0.0",
    "jest": "^26.6.3",
    "jest-expo": "^38.0.0",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react-native-storybook-loader": "^2.0.2",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "^16.13.1",
    "turtle-cli": "^0.20.7",
    "wait-on": "^5.0.1"
  }
}
